    "## 1. Global Submit Function"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "id": "fd3285f6",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-12-01T13:37:34.846886Z",
     "iopub.status.busy": "2025-12-01T13:37:34.846593Z",
     "iopub.status.idle": "2025-12-01T13:37:34.858114Z",
     "shell.execute_reply": "2025-12-01T13:37:34.857129Z"
    },
    "papermill": {
     "duration": 0.034192,
     "end_time": "2025-12-01T13:37:34.860015",
     "exception": false,
     "start_time": "2025-12-01T13:37:34.825823",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def checka(sub):\n",
    "    ref = pd.read_csv(\"/kaggle/input/Cinema_Audience_Forecasting_challenge/sample_submission/sample_submission.csv\")\n",
    "    \n",
    "    sub_ids = set(sub[\"ID\"].astype(str).str.strip())\n",
    "    ref_ids = set(ref[\"ID\"].astype(str).str.strip())\n",
    "    \n",
    "    print(\"Same IDs? →\", sub_ids == ref_ids)\n",
    "    if(sub_ids == ref_ids):\n",
    "        return True\n",
    "    print(\"Missing in submission : \", ref_ids - sub_ids)\n",
    "    print(\"Extra in submission   : \", sub_ids - ref_ids)\n",
    "    return False\n",
    "\n",
    "def now_submit(finish, y_pred, id_col='book_theater_id', \n",
    "               date_col='show_date', year_col='year', month_col='month', day_col='day',\n",
    "               filename='subm.csv'):\n",
    "    \"\"\"\n",
    "    Build submission DataFrame with ID = \"<book_theater_id>_YYYY-MM-DD\" and audience_count = y_pred.\n",
    "    \n",
    "    finish : DataFrame\n",
    "        Should contain either:\n",
    "         - a datetime column `date_col` (preferred), OR\n",
    "         - integer columns `year_col`, `month_col`, `day_col`.\n",
    "        And must contain `id_col`.\n",
    "    y_pred : array-like\n",
    "        Predictions (1-D) same length as finish.\n",
    "    \"\"\"\n",
    "    # convert preds to 1-D numpy array\n",
    "    preds = np.asarray(y_pred).reshape(-1)\n",
    "    \n",
    "    if len(preds) != len(finish):\n",
    "        raise ValueError(f\"Length mismatch: finish has {len(finish)} rows, but y_pred has {len(preds)} entries.\")\n",
    "    \n",
    "    df = finish.copy().reset_index(drop=True)\n",
    "    \n",
    "    # Ensure ID column exists and is string\n",
    "    if id_col not in df.columns:\n",
    "        raise KeyError(f\"{id_col} not found in finish\")\n",
    "    df[id_col] = df[id_col].astype(str)\n",
    "    \n",
    "    df['year']= 2024\n",
    "    if date_col in df.columns:\n",
    "        # ensure datetime dtype\n",
    "        df[date_col] = pd.to_datetime(df[date_col])\n",
    "        date_str = df[date_col].dt.strftime('%Y-%m-%d')\n",
    "    else:\n",
    "        # Otherwise require year/month/day columns\n",
    "        if not ({year_col, month_col, day_col} <= set(df.columns)):\n",
    "            raise KeyError(f\"Either '{date_col}' or all of '{year_col}','{month_col}','{day_col}' must be present in finish\")\n",
    "        # convert to ints then to zero-padded strings\n",
    "        yr = df[year_col].astype(int).astype(str)\n",
    "        mo = df[month_col].astype(int).apply(lambda x: f\"{x:02d}\")\n",
    "        da = df[day_col].astype(int).apply(lambda x: f\"{x:02d}\")\n",
    "        date_str = yr + \"-\" + mo + \"-\" + da\n",
    "    \n",
    "    # Build ID\n",
    "    ids = df[id_col] + \"_\" + date_str\n",
    "    \n",
    "    submission = pd.DataFrame({\n",
    "        'ID': ids,\n",
    "        'audience_count': preds\n",
    "    })\n",
    "    checka(submission)\n",
    "    submission.to_csv(filename, index=False)\n",
    "    return submission.head(2)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "87b07de2",
   "metadata": {
    "papermill": {
