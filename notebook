    "!pip install xgboost --quiet\n",
    "from xgboost import XGBRegressor\n",
    "\n",
    "xgb_model = XGBRegressor(\n",
    "    n_estimators=1300,\n",
    "    learning_rate=0.02,\n",
    "    max_depth=8,\n",
    "    subsample=0.8,\n",
    "    colsample_bytree=0.8,\n",
    "    objective='reg:squarederror',\n",
    "    random_state=42,\n",
    "    tree_method='hist',\n",
    "    gamma=0.3,\n",
    "    reg_alpha=1e-3,\n",
    ")\n",
    "\n",
    "xgb_model.fit(\n",
    "    X_train, Y_train,\n",
    "    eval_set=[(X_test, Y_test)],\n",
