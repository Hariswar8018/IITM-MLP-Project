   "source": [
    "## 5. Ordinal Encoding"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "112eabec",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-12-01T13:37:33.018322Z",
     "iopub.status.busy": "2025-12-01T13:37:33.017986Z",
     "iopub.status.idle": "2025-12-01T13:37:33.447102Z",
     "shell.execute_reply": "2025-12-01T13:37:33.445955Z"
    },
    "papermill": {
     "duration": 0.449058,
     "end_time": "2025-12-01T13:37:33.448659",
     "exception": false,
     "start_time": "2025-12-01T13:37:32.999601",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "✅ SUCCESS: All Test IDs exist in Train!\n"
     ]
    }
   ],
   "source": [
    "from sklearn.preprocessing import OrdinalEncoder\n",
    "\n",
    "# for now book_theater_id is the only cat column here\n",
    "cat = ['book_theater_id']\n",
    "\n",
    "# copy just to be sure\n",
    "final_test = test_data.copy()\n",
    "\n",
    "check_miss_id(df)\n",
    "\n",
    "df2=df.copy()\n",
    "ohe_dict = {}\n",
    "\n",
    "for ca in cat:\n",
    "    oh = OrdinalEncoder()\n",
    "    df2[ca]=oh.fit_transform(df2[[ca]])\n",
    "    final_test[ca]=oh.transform(final_test[[ca]])\n",
    "\n",
    "    ##saving encoded so that i could fetch later\n",
    "    ohe_dict[ca]=oh"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "2d89baef",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-12-01T13:37:33.484492Z",
     "iopub.status.busy": "2025-12-01T13:37:33.484159Z",
     "iopub.status.idle": "2025-12-01T13:37:33.505228Z",
     "shell.execute_reply": "2025-12-01T13:37:33.504219Z"
    },
    "papermill": {
     "duration": 0.04035,
     "end_time": "2025-12-01T13:37:33.506579",
     "exception": false,
     "start_time": "2025-12-01T13:37:33.466229",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [
