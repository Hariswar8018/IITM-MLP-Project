   "source": [
    "def add_day_week_year_too(dfs):\n",
    "    dfs['day'] = dfs['show_date'].dt.day\n",
    "    dfs['week_of_year'] = dfs['show_date'].dt.isocalendar().week.astype(int)\n",
    "    dfs.drop(columns=['show_date'],inplace=True)\n",
    "    dfs.fillna(0)\n",
    "    return dfs\n",
    "\n",
    "X_train = add_day_week_year_too(X_train)\n",
    "X_test = add_day_week_year_too(X_test)\n",
    "\n",
    "X_train.head(2)"
   ]
  },
