       "      <td>37.121711</td>\n",
       "    </tr>\n",
       "    <tr>\n",
       "      <th>1</th>\n",
       "      <td>book_00001</td>\n",
       "      <td>3</td>\n",
       "      <td>5</td>\n",
       "      <td>1</td>\n",
       "      <td>2</td>\n",
       "      <td>9</td>\n",
       "      <td>0.0</td>\n",
       "      <td>50.0</td>\n",
       "      <td>30.0</td>\n",
       "      <td>2.0</td>\n",
       "      <td>40.666667</td>\n",
       "      <td>24.969315</td>\n",
       "      <td>34.537049</td>\n",
       "      <td>36.298432</td>\n",
       "      <td>37.121711</td>\n",
       "    </tr>\n",
       "  </tbody>\n",
       "</table>\n",
       "</div>"
      ],
      "text/plain": [
       "  book_theater_id  month  day_of_week  is_weekend  day  week_of_year  lag_1  \\\n",
       "0      book_00001      3            4           0    1             9   22.0   \n",
       "1      book_00001      3            5           1    2             9    0.0   \n",
       "\n",
       "   lag_7  lag_14  lag_30  rolling_mean_7  rolling_std_7      ema_7     ema_14  \\\n",
       "0   28.0    34.0    46.0       38.857143      23.291118  34.537049  36.298432   \n",
       "1   50.0    30.0     2.0       40.666667      24.969315  34.537049  36.298432   \n",
       "\n",
       "      ema_30  \n",
       "0  37.121711  \n",
       "1  37.121711  "
      ]
     },
     "execution_count": 25,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "def make_features_for_test(\n",
    "        train_hist,\n",
    "        test_df,\n",
    "        lag_list=[1,7,14,30,],\n",
    "        roll_windows=[7],\n",
    "        ema_spans=[7,14,30]\n",
    "    ):\n",
    "    \"\"\"\n",
    "    Create test features identical to the training features.\n",
    "    Uses concatenated (train + test) so that lags come from history.\n",
    "    \"\"\"\n",
    "\n",
    "    # ---- 1) Convert to datetime ----\n",
    "    train_hist['show_date'] = pd.to_datetime(train_hist['show_date'])\n",
    "    test_df['show_date'] = pd.to_datetime(test_df['show_date'])\n",
    "\n",
    "    # ---- 2) Tag test rows ----\n",
    "    train_hist['_is_test'] = False\n",
    "    test_df['_is_test'] = True\n",
    "\n",
    "    # ---- 3) Concatenate ----\n",
    "    df = pd.concat([train_hist, test_df], ignore_index=True)\n",
    "    df = df.sort_values(['book_theater_id', 'show_date']).reset_index(drop=True)\n",
    "\n",
    "    g = df.groupby('book_theater_id', group_keys=False)\n",
    "\n",
    "    # ----------------------------------------------------\n",
    "    #                LAG FEATURES\n",
    "    # ----------------------------------------------------\n",
    "    for lag in lag_list:\n",
    "        df[f'lag_{lag}'] = g['audience_count'].shift(lag)\n",
    "\n",
    "    # ----------------------------------------------------\n",
    "    #             ROLLING FEATURES (Shifted)\n",
    "    # ----------------------------------------------------\n",
    "    for win in roll_windows:\n",
    "        df[f'rolling_mean_{win}'] = g['audience_count'].shift(1).transform(\n",
    "            lambda x: x.rolling(win, min_periods=1).mean()\n",
    "        )\n",
    "        df[f'rolling_std_{win}'] = g['audience_count'].shift(1).transform(\n",
    "            lambda x: x.rolling(win, min_periods=1).std()\n",
    "        )\n",
    "\n",
    "    # ----------------------------------------------------\n",
    "    #                     EMA\n",
    "    # ----------------------------------------------------\n",
    "    for span in ema_spans:\n",
    "        df[f'ema_{span}'] = g['audience_count'].shift(1).transform(\n",
    "            lambda x: x.ewm(span=span, adjust=False).mean()\n",
    "        )\n",
    "\n",
    "    # ----------------------------------------------------\n",
    "    #          DATE FEATURES (Same as training)\n",
    "    # ----------------------------------------------------\n",
    "    df['month'] = df['show_date'].dt.month\n",
    "    df['day_of_week'] = df['show_date'].dt.dayofweek\n",
    "    df['is_weekend'] = df['day_of_week'].isin([5,6]).astype(int)\n",
    "    df['day'] = df['show_date'].dt.day\n",
    "    df['week_of_year'] = df['show_date'].dt.isocalendar().week.astype(int)\n",
    "\n",
    "    # ---- Extract only test rows ----\n",
    "    test_features = df[df['_is_test']].copy().reset_index(drop=True)\n",
    "\n",
    "    # ---- Select ONLY existing columns (safe mode) ----\n",
    "    keep_cols = [\n",
    "        'book_theater_id',\n",
    "        'month','day_of_week','is_weekend','day','week_of_year','year',\n",
    "        # lags\n",
    "        *[f'lag_{l}' for l in lag_list],\n",
    "        # rolling\n",
    "        *[f'rolling_mean_{w}' for w in roll_windows],\n",
    "        *[f'rolling_std_{w}' for w in roll_windows],\n",
    "        # ema\n",
    "        *[f'ema_{s}' for s in ema_spans]\n",
    "    ]\n",
    "\n",
    "    keep_cols = [c for c in keep_cols if c in test_features.columns]\n",
    "\n",
    "    X_test = test_features[keep_cols].fillna(0)\n",
    "\n",
    "    return X_test\n",
    "\n",
    "X_test_features = make_features_for_test(train_hist=agg_df, test_df=test_data)\n",
    "X_test_features.head(2)"
   ]
  },
  {
