    "missing_count = visit_book['show_date'].isna().sum()\n",
    "print(f\"Total Missing show_date is {missing_count}\")\n",
    "\n",
    "visit_book['show_date'] = pd.to_datetime(visit_book['show_date'], errors='coerce')\n",
    "\n",
    "visit_book['show_datetime'] = pd.to_datetime(visit_book['show_datetime'], errors='coerce')\n",
    "visit_book.head(2)"
