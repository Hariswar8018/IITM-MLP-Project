     ]
    }
   ],
   "source": [
    "def count_duplicates(df1):\n",
    "    '''\n",
    "    Function to count duplicate book_theater_id\n",
    "    This is done so that each book theater id and show date should \n",
    "    have 1 Unique column only\n",
    "    '''\n",
    "    duplicate_pairs = df1.duplicated(subset=['book_theater_id', 'show_date'], keep=False)\n",
    "    dup_df = df1[duplicate_pairs].sort_values(['book_theater_id', 'show_date'])\n",
    "    count_dups = dup_df.shape[0]\n",
    "    print(f\"Shape of df is {df1.shape}\")\n",
    "    if count_dups == 0:\n",
    "        print(\"✅ No duplicate (book_theater_id, show_date) pairs found.\")\n",
    "    else:\n",
    "        print(f\"❌ {count_dups} duplicate rows found for (book_theater_id, show_date) pairs.\")\n",
    "\n",
    "count_duplicates(df)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "07cce1d3",
