    "def ids_in_train_not_in_test(test_df, train_df, column='book_theater_id'):\n",
    "    test_ids = set(test_df[column].dropna().unique())\n",
    "    train_ids = set(train_df[column].dropna().unique())\n",
    "\n",
    "    extra = train_ids - test_ids    \n",
    "    print(f\"Found {len(extra)} IDs in Train that do NOT appear in Test.\")\n",
    "    return extra\n",
    "\n",
    "check_miss_id(df)\n",
    "\n",
    "# Get extra ids\n",
    "extra_ids = ids_in_train_not_in_test(test, df, 'book_theater_id')\n",
    "\n",
    "# get only the Rows that appears in test ids\n",
    "train_clean = df[~df['book_theater_id'].isin(extra_ids)].copy()\n",
    "\n",
    "# check again\n",
    "check_miss_id(train_clean)\n",
    "extra_ids = ids_in_train_not_in_test(test, train_clean, 'book_theater_id')"
   ]
