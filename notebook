      ],
      "text/plain": [
       "  book_theater_id  show_date  audience_count  tickets_booked  tickets_sold  \\\n",
       "0      book_00001 2023-01-13            50.0             0.0           0.0   \n",
       "1      book_00001 2023-01-14            64.0             0.0           0.0   \n",
       "\n",
       "   month  day_of_week  is_weekend  lag_1  lag_7  lag_14  lag_30  \\\n",
       "0      1            4           0    NaN    NaN     NaN     NaN   \n",
       "1      1            5           1   50.0    NaN     NaN     NaN   \n",
       "\n",
       "   rolling_mean_7  rolling_std_7  ema_7  ema_14  ema_30  \n",
       "0             NaN            NaN    NaN     NaN     NaN  \n",
       "1            50.0            NaN   50.0    50.0    50.0  "
      ]
     },
     "execution_count": 17,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "df=train_clean.copy()\n",
    "\n",
    "# forcing to datetime in case\n",
    "df['show_date'] = pd.to_datetime(df['show_date'])\n",
    "\n",
    "# Sort and reset index for each theater sepeartely \n",
    "# and show_date → chronologically\n",
    "df = df.sort_values(['book_theater_id', 'show_date']).reset_index(drop=True)\n",
    "\n",
    "#group by theater so that lags and rolling windows are computed within each theater only\n",
    "g = df.groupby('book_theater_id')\n",
    "\n",
    "# Lags shift the target variable backward in time. show we take 1, 7, 14, 30 lags\n",
    "# These lags capture short-term and long-term seasonality/behavior. \n",
    "df['lag_1']   = g['audience_count'].shift(1)\n",
    "\n",
    "df['lag_7']   = g['audience_count'].shift(7)\n",
    "df['lag_14']  = g['audience_count'].shift(14)\n",
    "\n",
    "df['lag_30']  = g['audience_count'].shift(30)\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# Rolling Window to reveal trends \n",
    "# Compute the average audience over these 7 days\n",
    "df['rolling_mean_7']  = g['audience_count'].shift(1).transform(lambda x: x.rolling(7,  min_periods=1).mean())\n",
    "df['rolling_std_7']   = g['audience_count'].shift(1).transform(lambda x: x.rolling(7,  min_periods=1).std())\n",
    "\n",
    "\n",
    "# Exponential Moving Averages\n",
    "# EMAs give more weight to recent observations\n",
    "df['ema_7']   = g['audience_count'].shift(1).transform(lambda x: x.ewm(span=7,  adjust=False).mean())\n",
    "df['ema_14']   = g['audience_count'].shift(1).transform(lambda x: x.ewm(span=14,  adjust=False).mean())\n",
    "df['ema_30']  = g['audience_count'].shift(1).transform(lambda x: x.ewm(span=30, adjust=False).mean())\n",
    "\n",
    "#check if there is missing ids\n",
    "check_miss_id(df)\n",
    "\n",
    "df.head(2)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "591d05c0",
   "metadata": {
    "papermill": {
     "duration": 0.016962,
     "end_time": "2025-12-01T13:37:32.982456",
     "exception": false,
     "start_time": "2025-12-01T13:37:32.965494",
     "status": "completed"
