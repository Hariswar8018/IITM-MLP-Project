    "cine_full = cine_pos.merge(theater_relation, on = \"cine_theater_id\")\n",
    "\n",
    "cine_full[\"show_date\"] = pd.to_datetime(cine_full[\"show_datetime\"]).dt.date\n",
    "cine_daily = (\n",
    "    cine_full\n",
    "    .groupby([\"book_theater_id\", \"show_date\"], as_index=False)\n",
    "    .agg({\n",
    "        \"tickets_sold\": \"sum\",\n",
    "    })\n",
    ")\n",
    "cine_daily[\"show_date\"] = pd.to_datetime(cine_daily[\"show_date\"])\n",
    "cine_daily.head(2)"
