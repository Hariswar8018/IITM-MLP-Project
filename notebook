   "source": [
    "## 0. Handle NA Values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "id": "37c9215b",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-12-01T13:38:44.312063Z",
     "iopub.status.busy": "2025-12-01T13:38:44.311706Z",
     "iopub.status.idle": "2025-12-01T13:38:44.386230Z",
     "shell.execute_reply": "2025-12-01T13:38:44.385143Z"
    },
    "papermill": {
     "duration": 0.09781,
     "end_time": "2025-12-01T13:38:44.388518",
     "exception": false,
     "start_time": "2025-12-01T13:38:44.290708",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def fill_na(df):\n",
    "    df = df.copy()\n",
    "\n",
    "    zero_fill_cols = [col for col in df.columns \n",
    "                      if any(key in col.lower() for key in ['lag', 'rolling', 'ema'])]\n",
    "\n",
    "    for col in zero_fill_cols:\n",
    "        df[col] = df[col].fillna(0)\n",
    "\n",
    "    numeric_cols = df.select_dtypes(include='number').columns.difference(zero_fill_cols)\n",
    "\n",
    "    for col in numeric_cols:\n",
    "        df[col] = df[col].fillna(df[col].mean())\n",
    "\n",
    "    object_cols = df.select_dtypes(include=['object', 'category']).columns\n",
    "\n",
    "    for col in object_cols:\n",
    "        if df[col].mode().empty:      # rare case: all NaN\n",
    "            df[col] = df[col].fillna('Unknown')\n",
    "        else:\n",
    "            df[col] = df[col].fillna(df[col].mode()[0])\n",
    "\n",
    "    return df\n",
    "\n",
    "X_train = fill_na(X_train)\n",
    "X_test = fill_na(X_test)\n",
    "ft = fill_na(ft)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "d9b3d5e4",
   "metadata": {
    "papermill": {
