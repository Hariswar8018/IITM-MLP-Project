   "source": [
    "visit_book = visits.merge(booknow,on='book_theater_id',how='outer')\n",
    "\n",
    "# Checing nulls in value\n",
    "check_miss_id(visits)\n",
    "check_miss_id(booknow)\n",
    "\n",
    "# After merging all ids exist \n",
    "check_miss_id(visit_book)\n",
    "visit_book.head(2)"
   ]
