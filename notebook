   "source": [
    "def check_miss_id( train_df, column='book_theater_id'):\n",
    "    \"\"\"\n",
    "    Compare unique IDs in Test exist in Train. Just to throw away non trainale ids\n",
    "    \"\"\"\n",
    "    \n",
    "    test_ids = set(test_data[column].unique())\n",
    "    train_ids = set(train_df[column].unique())\n",
    "    \n",
    "    missing = test_ids - train_ids\n",
    "    \n",
    "    if len(missing) == 0:\n",
    "        print(\"✅ SUCCESS: All Test IDs exist in Train!\")\n",
    "    else:\n",
    "        print(f\"❌ {len(missing)} IDs from Test are missing in Train:\")        "
   ]
  },
