['show_date'].dt.month\n    concat['day_of_week'] = concat['show_date'].dt.dayofweek\n    concat['is_weekend'] = concat['day_of_week'].isin([5,6]).astype(int)\n    concat['day'] = concat['show_date'].dt.day\n    concat['week_of_year'] = concat['show_date'].dt.isocalendar().week.astype(int)\n    concat['year'] = concat['show_date'].dt.year\n    '''\n    # If test has no tickets_booked/sold and you used them in train, create placeholder zeros\n    if 'tickets_booked' not in concat.columns:\n        concat['tickets_booked'] = 0\n    if 'tickets_sold' not in concat.columns:\n        concat['tickets_sold'] = 0\n    \n    # lag tickets if you used them (shifted)\n    concat['lag_tickets_booked_1'] = concat.groupby('book_theater_id')['tickets_booked'].shift(1)\n    concat['lag_tickets_sold_1']   = concat.groupby('book_theater_id')['tickets_sold'].shift(1)\n    '''\n    # mark and extract test rows (preserve original order)\n    test_features = concat[concat['_is_test']].copy()\n    test_features = test_features.reset_index(drop=True)\n    \n    # list of feature columns to keep — match what you used to train\n    feature_cols = [\n        'book_theater_id','month','day_of_week','is_weekend',\n        'lag_1','lag_7','lag_14','lag_30',\n        f'rolling_mean_{roll_win}', f'rolling_std_{roll_win}',\n        'ema_7','ema_14','ema_30',\n        'lag_tickets_booked_1','lag_tickets_sold_1',\n        'day','week_of_year','year'\n    ]\n    # keep only columns that exist (defensive)\n    feature_cols = [c for c in feature_cols if c in test_features.columns]\n    X_test = test_features[feature_cols].copy()\n    \n    X_test = X_test.fillna(0)\n    \n    return X_test\n\nX_test_features = make_features_for_test(train_hist=agg_df, test_df=test_data)\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-11-14T06:14:24.005047Z","iopub.execute_input":"2025-11-14T06:14:24.005397Z","iopub.status.idle":"2025-11-14T06:14:24.317092Z","shell.execute_reply.started":"2025-11-14T